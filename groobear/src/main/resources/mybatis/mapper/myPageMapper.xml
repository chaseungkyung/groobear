<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sp.app.model.MyPageMapper">
	<select id="workList" parameterType="com.sp.app.model.MyPage">
		SELECT workIdx, e.empIdx, workDate, loginTime
		FROM workLog w
		JOIN employee e ON e.empIdx = w.empIdx
	</select>
</mapper>